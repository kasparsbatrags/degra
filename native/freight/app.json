{
  "expo": {
    "name": "Krava",
    "slug": "Kravas uzskaite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "backgroundColor": "#161622",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "lv.degra.accounting.freight"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#161622"
      },
      "package": "lv.degra.accounting.freight"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "freight",
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Poppins-Black.ttf",
        "family": "Poppins-Black"
      },
      {
        "asset": "./assets/fonts/Poppins-Bold.ttf",
        "family": "Poppins-Bold"
      },
      {
        "asset": "./assets/fonts/Poppins-ExtraBold.ttf",
        "family": "Poppins-ExtraBold"
      },
      {
        "asset": "./assets/fonts/Poppins-ExtraLight.ttf",
        "family": "Poppins-ExtraLight"
      },
      {
        "asset": "./assets/fonts/Poppins-Light.ttf",
        "family": "Poppins-Light"
      },
      {
        "asset": "./assets/fonts/Poppins-Medium.ttf",
        "family": "Poppins-Medium"
      },
      {
        "asset": "./assets/fonts/Poppins-Regular.ttf",
        "family": "Poppins-Regular"
      },
      {
        "asset": "./assets/fonts/Poppins-SemiBold.ttf",
        "family": "Poppins-SemiBold"
      },
      {
        "asset": "./assets/fonts/Poppins-Thin.ttf",
        "family": "Poppins-Thin"
      }
    ]
  }
}
