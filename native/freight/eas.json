{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
	  "env": {
	 	"APP_ENV": "preview"
	  }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
	  "env": {
		"APP_ENV": "production"
	  }
    }
  },
  "submit": {
    "production": {}
  }
}
