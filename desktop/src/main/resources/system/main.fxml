<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<BorderPane prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lv.degra.accounting.desktop.system.controller.MainController">
    <left>
        <VBox />
    </left>
    <center>
        <BorderPane>
            <center>
                <BorderPane fx:id="mainBorderPane">
                    <center>
                        <StackPane />
                    </center>
                    <left>
                        <AnchorPane prefHeight="800.0" prefWidth="139.0" BorderPane.alignment="CENTER">
                            <children>
                                <Button fx:id="documentsButton" mnemonicParsing="false" onAction="#openDocumentList" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">Dokumenti
                                    <graphic>
                                        <FontIcon iconLiteral="mdi-file-document" iconSize="32" />
                                    </graphic>
                                </Button>
                                <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#openSettings" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">IestatÄ«jumi
                                    <graphic>
                                        <FontIcon iconLiteral="mdi-settings" iconSize="32" />
                                    </graphic>
                                </Button>
                            </children>
                        </AnchorPane>
                    </left>
                </BorderPane>
            </center>
        </BorderPane>
    </center>
</BorderPane>
